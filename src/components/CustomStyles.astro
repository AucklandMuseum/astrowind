---
import '@fontsource/inter/variable.css';
import '/fonts/gotham-narrow-500.woff';
import '/fonts/gotham-narrow-700.woff';
import '/fonts/gotham-narrow-ssm-400.woff'
import '/fonts/gotham-narrow-ssm-700.woff'
// Nunito
// Dosis
---

<style is:inline is:global>
      @font-face {
	font-family: 'Gotham Narrow';
	src: url(/fonts/gotham-narrow-500.eot?#iefix) format("embedded-opentype"), url(/fonts/gotham-narrow-500.woff) format("woff");
	font-weight: 500;
	font-style: normal;
  }
  @font-face {
	font-family: 'Gotham Narrow';
	src: url(/fonts/gotham-narrow-700.eot?#iefix) format("embedded-opentype"), url(/fonts/gotham-narrow-700.woff) format("woff");
	font-weight: 700;
	font-style: normal;
  }
  @font-face {
	font-family: 'Gotham Narrow';
	src: url(/fonts/gotham-narrow-ssm-400.eot?#iefix) format("embedded-opentype"), url(/fonts/gotham-narrow-ssm-400.woff) format("woff");
	font-weight: 400;
	font-style: normal;
  }
  @font-face {
	font-family: 'Gotham Narrow';
	src: url(/fonts/gotham-narrow-ssm-700.eot?#iefix) format("embedded-opentype"), url(/fonts/gotham-narrow-ssm-700.woff) format("woff");
	font-weight: 700;
	font-style: normal;
  }
  :root {

    --aw-font-sans: 'Gotham Narrow';
    --aw-font-serif: var(--aw-font-sans);
    --aw-font-heading: var(--aw-font-sans);

    --aw-color-primary: rgb(30 64 175);
    --aw-color-secondary: rgb(30 58 138);
    --aw-color-accent: rgb(109 40 217);
    --aw-color-text-page: rgb(17 24 39);
    --aw-color-text-muted: rgb(75 85 99);
    --aw-color-bg-page: rgb(255 255 255);
  }
</style>
